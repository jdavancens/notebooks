    "def insertion_sort(A):\n",
    "    for i in range(2,len(A)):\n",
    "        current = A[i]\n",
    "        position = i\n",
    "        while position>0 and A[position-1]>current:\n",
    "            A[position] = position-1\n",
    "        A[position] = current"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "A = [5,2,3,6,1,2]\n",
    "insertion_sort(A)\n",
    "print(A)"
   ]
